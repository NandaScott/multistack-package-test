{
  "name": "nds-common",
  "version": "0.5.3",
  "description": "Testing to see how a multistack package works",
  "main": "./build/index.js",
  "types": "./build/index.d.js",
  "files": [
    "build/**/*"
  ],
  "exports": {
    ".": "./build/index.js",
    "./components": "./build/components",
    "./hooks": "./build/hooks",
    "./questions": "./build/questions",
    "./services": "./build/services"
  },
  "typesVersions": {
    "*": {
      ".": [
        "."
      ],
      "components": [
        "./build/components"
      ],
      "hooks": [
        "./build/hooks"
      ],
      "question": [
        "./build/question"
      ],
      "services": [
        "./build/services"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "pub:major": "npm version major && npm run build && npm publish",
    "pub:minor": "npm version minor && npm run build && npm publish",
    "pub:patch": "npm version patch && npm run build && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nanda Scott",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "axios": "^0.26.1",
    "currency-codes": "^2.1.0",
    "joi": "^17.6.0",
    "react-hook-form": "^7.30.0"
  }
}
